{
  "name": "json-schema-to",
  "version": "0.0.5",
  "description": "JSON-Schema To ≤GraphQL|Protobuf|Code≥.™",
  "main": "index.js",
  "bin": {
    "json-schema-to": "bin/cli"
  },
  "scripts": {
    "watch": "npm test -- -w",
    "test": "npm run test:unit --",
    "test:ci": "npm run coverage && npm run report --",
    "test:unit": "NODE_ENV=test _mocha --watch-extensions gql,json,proto --exit --recursive -bR spec 'test/**/*.test.js'",
    "coverage": "NODE_ENV=test istanbul cover --root lib -- _mocha --exit --recursive -bR spec 'test/**/*.test.js'",
    "report": "codecov --file=coverage/lcov.info -e TRAVIS_NODE_VERSION"
  },
  "keywords": [],
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "glob": "^7.1.2",
    "json-schema-ref-parser": "^5.1.2",
    "wargs": "^0.8.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.5",
    "grpc": "^1.13.1",
    "is-my-json-valid": "^2.17.2",
    "istanbul": "^0.4.5",
    "json-schema-faker": "^0.5.0-rc15",
    "mocha": "^5.2.0",
    "mock-fs": "^4.5.0",
    "testdouble": "^3.8.1"
  }
}
