{
  "name": "json-schema-to",
  "version": "0.0.1",
  "description": "JSON-Schema To ≤GraphQL|Protobuf|Code≥.™",
  "main": "index.js",
  "scripts": {
    "watch": "npm test -- -w",
    "test": "npm run test:unit --",
    "test:ci": "npm run coverage:unit && npm run report:lcov && npm run report:upload --",
    "test:unit": "NODE_ENV=test _mocha --exit --recursive -bR spec 'test/**/*.test.js'",
    "coverage": "nyc -x '**/fixtures/**' -x '**/fixtures.js' -x '**/*.test.js'",
    "coverage:unit": "npm run coverage -- npm run test:unit",
    "report": "nyc report -r html",
    "report:lcov": "nyc report -r text-lcov > coverage/lcov.info",
    "report:upload": "codecov --file=coverage/lcov.info -e TRAVIS_NODE_VERSION"
  },
  "keywords": [],
  "author": "Alvaro Cabrera <pateketrueke@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "chai": "^4.1.2",
    "codecov": "^3.0.4",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.0.5",
    "grpc": "^1.13.1",
    "is-my-json-valid": "^2.17.2",
    "json-schema-faker": "^0.5.0-rc15",
    "json-schema-ref-parser": "^5.1.1",
    "mocha": "^5.2.0",
    "mock-fs": "^4.5.0",
    "nyc": "^12.0.2",
    "testdouble": "^3.8.1"
  }
}
